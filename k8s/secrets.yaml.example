# Example secrets file - DO NOT commit actual secrets to git
# Create actual secrets using: kubectl create secret generic indostar-secrets --from-env-file=.env.prod -n indostar-naturals
apiVersion: v1
kind: Secret
metadata:
  name: indostar-secrets
  namespace: indostar-naturals
type: Opaque
stringData:
  DATABASE_URL: "postgresql://username:password@postgres-service:5432/indostar_naturals"
  REDIS_URL: "redis://:password@redis-service:6379/0"
  REDIS_PASSWORD: "your-redis-password"
  JWT_SECRET_KEY: "your-jwt-secret-key"
  RAZORPAY_KEY_ID: "rzp_live_xxxxxxxxxxxxx"
  RAZORPAY_KEY_SECRET: "your-razorpay-key-secret"
  RAZORPAY_WEBHOOK_SECRET: "your-razorpay-webhook-secret"
  TWILIO_ACCOUNT_SID: "ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
  TWILIO_AUTH_TOKEN: "your-twilio-auth-token"
  TWILIO_PHONE_NUMBER: "+1234567890"
  SENDGRID_API_KEY: "SG.xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
  SENDGRID_FROM_EMAIL: "noreply@indostarnaturals.com"
  GOOGLE_OAUTH_CLIENT_ID: "xxxxxxxxxxxxx.apps.googleusercontent.com"
  GOOGLE_OAUTH_CLIENT_SECRET: "your-google-oauth-client-secret"
  S3_BUCKET_NAME: "indostar-naturals-media-prod"
  S3_ACCESS_KEY: "AKIAXXXXXXXXXXXXXXXX"
  S3_SECRET_KEY: "your-s3-secret-key"
  CDN_BASE_URL: "https://d1234567890abc.cloudfront.net"
  SENTRY_DSN: "https://xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx@sentry.io/1234567"
  CELERY_BROKER_URL: "redis://:password@redis-service:6379/0"
  CELERY_RESULT_BACKEND: "redis://:password@redis-service:6379/1"
